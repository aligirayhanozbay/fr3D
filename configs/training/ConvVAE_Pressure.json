{
    "model": {
	"levels": 5,
	"base_filters": 16,
	"kernel_size": 3,
	"activation": "leaky_relu",
	"normalization": "batchnorm",
	"pre_activation": true,
	"residual_connection": true,
	"blocks_per_level": 3,
	"pool_size": 2,
	"bottleneck_size": 8192
    },
    "training": {
	"epochs": 200,
	"validation_steps": 10,
	"early_stopping": {"patience": 15, "verbose": true, "monitor": "val_loss"},
	"reduce_lr": {"patience": 7, "verbose": true, "min_lr": 1e-6, "monitor": "loss"},
	"loss": "mse",
	"metrics": ["mae", "mse"],
	"optimizer": {
		"class_name": "adam",
		"config": {
			"learning_rate": 1e-4
		}
	}
    },
    "dataset": {
	"batch_size": 30,
	"train_test_split": [0.80,0.20],
	"training_node": "pressure",
	"node_configurations":[
	    {
		"nodetype": "HDF5IODataset",
		"identifier": "full_field",
		"field": "full_field"
            },
	    {
		"nodetype": "keep_vars",
		"identifier": "pressure",
		"inputs": ["full_field"],
		"vars_to_keep": ["p"]
	    }
			       
	]
    }

}
